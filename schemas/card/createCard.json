                    },
                    "institutionId": {
                        "type": "string",
                        "pattern": "^\\d{9}$"
                    }
                },
                "additionalProperties": false,
                "required": ["bin", "institutionId"]
            },
            "CreateIndividualDebitCard": {
                "title": "Create Individual Debit Card",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
