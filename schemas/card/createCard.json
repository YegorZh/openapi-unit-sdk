                            "idempotencyKey": {
                                "type": "string",
                                "minLength": 1,
                                "maxLength": 255
                            },
                            "bin": {
                                "$ref": "#/components/schemas/BIN"
                            },
                            "cardQualifier": {
                                "type": "string",
                                "pattern": "^(\\d{3})|([ ]{3})$"
                            },
                            "cardDesignId": {
                                "type": "string",
                                "maxLength": 16
                            },
                            "additionalEmbossedText": {
                                "type": "string",
                                "maxLength": 26
                            },
                            "activeForOnlineUse": {
                                "type": "boolean"
                            },
                            "printOnlyBusinessName": {
                                "type": "boolean"
                            },
                            "expiryDate": {
                                "type": "string",
                                "pattern": "^(1[0-2]|0[1-9])/(\\d{2})$|^(\\d{4})-(1[0-2]|0[1-9])$|^(\\d{2})-(1[0-2]|0[1-9])$"
                            }
                        },
                        "additionalProperties": false
