{
    "components": {
        "schemas": {
            "patchCheckDeposit": {
                "title": "Patch Check Deposit",
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "default": "checkDeposit"
                    },
                    "attributes": {
                        "type": "object",
                        "properties": {
                            "amount": { "type": "integer", "minimum": 1 },
                            "tags": {
                                "$ref": "../tags.json#/components/schemas/tagsPatch"
                            }
                        },
                        "additionalProperties": false
                    }
                },
                "additionalProperties": false,
                "required": ["type", "attributes"]
            },
            "updateCheckDeposit": {
                "title": "Patch Check Deposit",
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/patchCheckDeposit"
                    }
                },
                "additionalProperties": false,
                "required": ["data"]
            }
        }
    }
}
